{
    "targets": {
        "token": {
            "outFile": "build/MyToken.wasm",
            "use": [
                "abort=src/token/index/abort"
            ]
        },
        "nft": {
            "outFile": "build/MyNFT.wasm",
            "use": [
                "abort=src/nft/index/abort"
            ]
        }
    },
    "options": {
        "sourceMap": false,
        "optimizeLevel": 3,
        "shrinkLevel": 1,
        "converge": true,
        "noAssert": false,
        "enable": [
            "sign-extension",
            "mutable-globals",
            "nontrapping-f2i",
            "bulk-memory",
            "simd",
            "reference-types",
            "multi-value"
        ],
        "runtime": "stub",
        "memoryBase": 0,
        "initialMemory": 1,
        "exportStart": "start",
        "transform": "@btc-vision/opnet-transform"
    }
}
